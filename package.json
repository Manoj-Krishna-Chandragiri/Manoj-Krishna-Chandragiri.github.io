{
  "name": "portfolio-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://manojkrishna.me",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "switch-gh-pages": "git checkout gh-pages",
    "switch-main": "git checkout main",
    "compare-branches": "git diff main...gh-pages",
    "compare-detailed": "git diff --stat main...gh-pages",
    "branch-history": "git log --graph --oneline --all",
    "branch-status": "git status",
    "recent-activity": "git for-each-ref --sort=-committerdate refs/heads/ --format='%(committerdate:short) %(refname:short)'",
    "compare-recent": "git log -p gh-pages@{4.minutes.ago}..gh-pages",
    "prepare-pr": "git push origin gh-pages && gh pr create --base main --head gh-pages",
    "push-history": "git reflog --date=relative gh-pages",
    "sync-branches": "git fetch origin && git merge origin/main && git push origin gh-pages",
    "view-pr": "gh pr list --base main --head gh-pages",
    "serve": "vite preview --host"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.20.0",
    "react-simple-typewriter": "^5.0.1",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gh-pages": "^6.3.0",
    "vite": "^4.4.5"
  }
}
